    <declare-styleable name="TCLayerOperationView">
        <attr format="reference" name="src"/>            <!-- 用于缩放旋转的图标 -->
        <attr format="boolean" name="editable"/>          <!-- 是否处于可编辑状态 -->
        <attr format="color" name="frameColor"/>         <!-- 边框颜色 -->
        <attr format="dimension" name="frameWidth"/>     <!-- 边框线宽度 -->
        <attr format="dimension" name="framePadding"/>   <!-- 边框与图片的间距 -->
        <attr format="float" name="degree"/>             <!-- 旋转角度 -->
        <attr format="float" name="scale"/>              <!-- 缩放比例 -->
        <attr format="reference" name="controlDrawable"/> <!-- 控制图标 -->
        <attr format="reference" name="editDrawable"/>   <!--编辑图标位置-->
        <attr format="reference" name="deleteDrawable"/> <!--删除图标位置-->
        <attr name="controlLocation">                     <!-- 控制图标的位置 -->
            <enum name="left_top" value="0"/>
            <enum name="right_top" value="1"/>
            <enum name="right_bottom" value="2"/>
            <enum name="left_bottom" value="3"/>
        </attr>
        <attr name="editLocation">                     <!-- 编辑图标的位置 -->
            <enum name="left_top" value="0"/>
            <enum name="right_top" value="1"/>
            <enum name="right_bottom" value="2"/>
            <enum name="left_bottom" value="3"/>
        </attr>
        <attr name="deleteLocation">                     <!-- 删除图标的位置 -->
            <enum name="left_top" value="0"/>
            <enum name="right_top" value="1"/>
            <enum name="right_bottom" value="2"/>
            <enum name="left_bottom" value="3"/>
        </attr>
    </declare-styleable>
</resources>