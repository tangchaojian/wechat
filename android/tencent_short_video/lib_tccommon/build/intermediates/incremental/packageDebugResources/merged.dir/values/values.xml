    <style name="ConfirmDialogStyle" parent="Base.Theme.AppCompat.Light.Dialog">
        <item name="windowMinWidthMinor">90%</item>
        <item name="android:textColorPrimary">#000000</item>
        <item name="colorAccent">#0accac</item>
    </style>
    <style name="CustomEditText" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">@color/colorGray4</item>
        <item name="colorControlActivated">@color/colorAccent</item>
    </style>
    <style name="DialogFragmentStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
    <style name="NumberProgressBarStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="progress_max">100</item>
        <item name="progress_current">0</item>
        <item name="progress_unreached_color">#fff</item>
        <item name="progress_reached_color">@color/colorAccent</item>
        <item name="progress_text_size">10sp</item>
        <item name="progress_text_color">@color/colorAccent</item>
        <item name="progress_reached_bar_height">1.5dp</item>
        <item name="progress_unreached_bar_height">0.75dp</item>
    </style>
    <style name="loading_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <declare-styleable name="NumberProgressBar">
        <attr format="integer" name="progress_current"/>
        <attr format="integer" name="progress_max"/>
        <attr format="color" name="progress_unreached_color"/>
        <attr format="color" name="progress_reached_color"/>
        <attr format="dimension" name="progress_reached_bar_height"/>
        <attr format="dimension" name="progress_unreached_bar_height"/>
        <attr format="dimension" name="progress_text_size"/>
        <attr format="color" name="progress_text_color"/>
        <attr format="dimension" name="progress_text_offset"/>
        <attr format="enum" name="progress_text_visibility">
            <enum name="visible" value="0"/>
            <enum name="invisible" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="RangeSeekBar">
        <attr format="reference" name="rsb_pointerBackground"/>
        <attr format="color" name="rsb_backgroundColor"/>
        <attr format="color" name="rsb_progressColor"/>
        <attr format="integer" name="rsb_range"/>
    </declare-styleable>
    <declare-styleable name="RangeSlider">
        <attr format="dimension" name="lineHeight"/>
        <attr format="dimension" name="thumbWidth"/>
        <attr format="color" name="lineColor"/>
        <attr format="color" name="maskColor"/>
        <attr format="integer" name="tickCount"/>
        <attr format="integer" name="leftThumbIndex"/>
        <attr format="integer" name="rightThumbIndex"/>
        <attr format="reference" name="leftThumbDrawable"/>
        <attr format="reference" name="rightThumbDrawable"/>
        <attr format="reference" name="midThumbDrawable"/>
    </declare-styleable>
    <declare-styleable name="SwipeMenuLayout">
        <attr format="reference" name="leftViewId"/>
        <attr format="reference" name="rightViewId"/>
        <attr format="reference" name="contentViewId"/>
    </declare-styleable>
    <declare-styleable name="TCActivityTitle">
        <!-- 标题文案 -->
        <attr format="string" name="titleText"/>
        <!-- 是否有back按钮 -->
        <attr format="boolean" name="canBack"/>
        <!-- back按钮文案 -->
        <attr format="string" name="backText"/>
        <!-- 右侧更多功能按钮文字 -->
        <attr format="string" name="moreText"/>
    </declare-styleable>
    <declare-styleable name="TCPointSeekBar">
        <attr format="reference" name="psb_thumbBackground"/>
        <attr format="color" name="psb_backgroundColor"/>
        <attr format="color" name="psb_progressColor"/>
        <attr format="integer" name="psb_progress"/>
        <attr format="integer" name="psb_max"/>
        <attr format="dimension" name="psb_progressHeight"/>
    </declare-styleable>
    <declare-styleable name="TCReversalSeekBar">
        <attr format="reference" name="rs_pointerBackground"/>
        <attr format="color" name="rs_backgroundColor"/>
        <attr format="color" name="rs_progressColor"/>
        <attr format="float" name="rs_progress"/>
    </declare-styleable>
    <declare-styleable name="TCSeekbarWithText">
        <attr format="dimension" name="textsize"/>
        <attr format="color" name="textcolor"/>
        <attr format="reference" name="img"/>
    </declare-styleable>
    <declare-styleable name="TCTouchSeekBar">
        <!--默认刻度图片-->
        <attr format="reference" name="tsb_dotDefault"/>
        <!--选中的刻度图片-->
        <attr format="reference" name="tsb_dotChecked"/>
    </declare-styleable>
    <declare-styleable name="absProgressBar">
        <attr format="integer" name="progress"/>
        <attr format="reference|color" name="backgroundColor"/>
        <attr format="reference|color" name="textColor"/>
    </declare-styleable>
    <declare-styleable name="floatTextProgressBar">
        <attr format="reference|color" name="triangleColor"/>
        <attr format="reference|color" name="rectColor"/>
        <attr format="reference|color" name="fillColor"/>
    </declare-styleable>
    <declare-styleable name="progressBar">
        <attr format="reference|color" name="startFillColor"/>
        <attr format="reference|color" name="middleFillColor"/>
        <attr format="reference|color" name="endFillColor"/>
        <attr format="reference|color" name="arrowPointColor"/>
        <attr format="dimension|reference" name="arrowPointRadius"/>
    </declare-styleable>
